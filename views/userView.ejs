<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Details</title>
    <link rel="stylesheet" href="/assets/bootstrap/css/bootstrap.min.css">
    <style media="screen">
    p.text-uppercase.text-right.author{
margin-right:80px;
margin-top:100px;
}
div.outercontainer{
  margin-top: 50px;
}
div.row.cgpa{
  margin-top: 20px;
}
</style>
</head>
<body>
  <div class="outercontainer">
    <div class="container">
        <div class="row">
            <div class="col-md-6"><img src="/assets/img/logo_chitkara_black.png" width="200" height="50"></div>
            <div  class="col-md-6">
                <p style="vertical-align:middle;padding:5px;text-align:right;">Date : _______________</p>
            </div>
            <% var test=student; %>
            <%= student %>
      <div class="container">
         <div class="row">
             <%= test.dob%>
             <div class="col-md-12">
                 <h4>Personal Details:</h4></div>
             <div class="col-md-3">
                 <p>Full Name :</p>
                 <p class="attribvalue"><%= student.firstName %></p>
             </div>
             <div class="col-md-3">
                 <p>Mobile No :</p>
                 <p class="attribvalue"><%=student.mobileNo%></p>
                 
                 <%=student.mobileNo%>
                 
             </div>
             <div class="col-md-3">
                 <p>Gender :</p>
                 <p class="attribvalue">_________________________ </p>
             </div>
             <div class="col-md-3">
                 <p>Date of Birth :</p>
                 <p class="attribvalue">_________________________ </p>
             </div>
         </div>
     </div>

     <div class="container">
         <div class="row">
             <div class="col-md-3">
                 <p>E-Mail ID :</p>
                 <p class="attribvalue">_________________________ </p>
             </div>
         </div>
     </div>
   </div>
     <div class="container">
         <div class="row">
             <div class="col-md-12">
                 <h4>Address :</h4></div>
             <div class="col-md-3">
                 <p>Permanent Address :</p>
                 <p class="attribvalue">_________________________ </p>
             </div>
             <div class="col-md-3">
                 <p>Pincode :</p>
                 <p class="attribvalue">_________________________ </p>
             </div>
         </div>
     </div>
     <div class="container">
         <div class="row">
             <div class="col-md-12">
                 <h4>Education &amp; Qualification</h4>
                 <div class="table-responsive">
                     <table class="table">
                         <thead>
                             <tr>
                                 <th>School Name</th>
                                 <th>Type </th>
                                 <th>Board </th>
                                 <th>Year of Passing</th>
                                 <th>CGPA.Obt </th>
                                 <th>CGPA.Max </th>
                             </tr>
                         </thead>
                         <tbody>
                             <tr>
                                 <td>Cell 1</td>
                                 <td>Cell 2</td>
                                 <td>Cell 3</td>
                                 <td>Cell 4</td>
                                 <td>Cell 5</td>
                                 <td>Cell 6</td>
                             </tr>
                         </tbody>
                     </table>
                 </div>
             </div>
         </div>
     </div>
     <div class="container">
         <div class="row">
             <div class="col-md-12">
                 <h4>Technical Skills &amp; Projects </h4>
                 <ul>
                     <li>Item 1</li>
                     <li>Item 2</li>
                     <li>Item 3</li>
                     <li>Item 4</li>
                 </ul>
             </div>
         </div>
     </div>
    </div>
    <script src="/assets/js/jquery.min.js"></script>
    <script src="/assets/bootstrap/js/bootstrap.min.js"></script>
</body>

</html>
